@echo off

java.exe @JAVA_FLAGS@ -cp timemine-windows.jar DataModel > NUL:
if errorlevel 64 goto 64
if errorlevel 32 goto 32
goto error

:64
  java.exe @JAVA_FLAGS@ -jar timemine-windows_64.jar --debug %*
  if errorlevel 64 goto 64
goto end

:32
  java.exe @JAVA_FLAGS@ -jar timemine-windows.jar --debug %*
  if errorlevel 64 goto 32
goto end

:error
  echo ERROR: cannot detect data model (32/64 bit) - VM failure?
goto end

:end
@echo on
